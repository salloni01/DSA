#include<stdio.h>
##include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {

   int n;
    scanf("%d",&n);
    int stack[n];
    for(int i=0;i<n;i++){
    scanf("%d",&stack[i]);
    }
    int a;
    for(int i=0;i<n;i++){
     for(int j=i+1;j<n;j++){
        if(stack[i]<stack[j])
        {
        a=stack[i];
        stack[i]=stack[j];
        stack[j]=a;
}
}
    }
     for(int i=0;i<n;i++){
    printf("%d ",stack[i]);
    }
    return 0;
}
